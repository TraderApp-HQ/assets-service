{
	"name": "traderapp-assets",
	"version": "1.0.0",
	"description": "",
	"main": "src/app.ts",
	"scripts": {
		"build": "concurrently \"rm -rf build/\" \"npx tsc\"",
		"start": "node build/src/app.js",
		"dev": "npx nodemon src/app.ts",
		"test": "npx jest",
		"db:init": "npx prisma migrate dev --name init",
		"init-exchanges": "npx ts-node src/scripts/initialize-exchanges-script",
		"init-coins": "npx ts-node src/scripts/initialize-coins-script",
		"init-binance": "npx ts-node src/scripts/initialize-binance-script",
		"init-kucoin": "npx ts-node src/scripts/initialize-kucoin-script",
		"init-coins-transfer": "npx ts-node src/scripts/transfer-coins-collection-script",
		"init-exchanges-transfer": "npx ts-node src/scripts/transfer-exchanges-collection-script",
		"init-currencies-transfer": "npx ts-node src/scripts/transfer-currencies-collection-script",
		"init-exchangepair-transfer": "npx ts-node src/scripts/transfer-exchangepair-unknown-coin.script",
		"prepare": "husky install",
		"codegen": "npx prisma generate",
		"migration": "npx prisma migrate deploy"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.627.0",
		"@aws-sdk/client-secrets-manager": "^3.437.0",
		"@prisma/client": "^5.7.1",
		"@traderapp/shared-resources": "^1.0.5",
		"axios": "^1.6.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"joi": "^17.7.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^8.5.1",
		"mongoose-sequence": "^6.0.1",
		"prisma": "^4.16.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.0",
		"uuid": "^10.0.0",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.15",
		"@types/jest": "^29.2.4",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/mongoose": "^5.11.97",
		"@types/mongoose-sequence": "^3.0.11",
		"@types/node": "^18.11.17",
		"@types/supertest": "^2.0.12",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.6",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"concurrently": "^7.6.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^24.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.6.0",
		"eslint-plugin-promise": "^6.1.1",
		"husky": "^8.0.0",
		"jest": "^29.3.1",
		"lint-staged": "^15.0.2",
		"prettier": "^2.8.1",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	}
